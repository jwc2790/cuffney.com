{
  "Version": "2008-10-17",
  "Statement": [
      {
          "Effect": "Allow",
          "Principal": {
              "AWS": "arn:aws:iam::cloudfront:user/CloudFront Origin Access Identity ${orign_access_identity_id}"
          },
          "Action": "s3:GetObject",
          "Resource": "arn:aws:s3:::${bucket}/*"
      }
  ]
}