{
  "name": "cuffney.com",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "NODE_PATH=src react-scripts start",
    "build": "NODE_PATH=src react-scripts build && react-snapshot",
    "test": "NODE_PATH=src react-scripts test --env=jsdom",
    "lint": "NODE_PATH=src eslint --fix src/**/*.js",
    "cover": "npm run test -- --coverage"
  },
  "dependencies": {
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-ga": "^2.5.7",
    "react-icons": "^3.7.0",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^3.0.1"
  },
  "devDependencies": {
    "@cuffney/eslint-config": "^1.0.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "chai": "^4.2.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "react-snapshot": "^1.3.0",
    "redux-mock-store": "^1.5.3",
    "sinon": "^7.3.2"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "statements": 50,
        "branches": 50,
        "lines": 50,
        "functions": 50
      }
    },
    "collectCoverageFrom": [
      "src/**/*.js",
      "!node_modules/",
      "!src/registerServiceWorker.js",
      "!src/setupTests.js",
      "!src/index.js"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
